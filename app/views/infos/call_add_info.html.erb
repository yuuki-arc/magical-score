
  <div class="panel panel-primary">
    <% @infos.each do |info| %>
      <table class="table table-striped table-hover table-condensed">
        <tbody>
          <% info[:mode].each do |mode| %>
            <tr class="<%= get_difficulty_text(mode) %>">
              <td><%= info[:title] %></td>
              <td><span class="label <%= get_difficulty_label(mode) %>"><%= info[mode][:star] %></span></td>
              <td style="width:30px;"><img src="<%= info[mode][:detail][:image] %>" width="25" height="26" border="0"></td>
              <td><%= info[mode][:detail][:rate] %></td>
              <td><%= info[mode][:detail][:point] %></td>
              <td><%= info[mode][:detail][:trial] %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    <% end %>
  </div>
